{
  "dependencies": {
    "typescript": "^5.4.5"
  },
  "name": "mytail",
  "version": "1.0.0",
  "description": "A Log monitoring and retrieval app",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.3"
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon src/server.ts",
